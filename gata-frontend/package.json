{
   "name": "gata-frontend",
   "version": "0.2.3",
   "private": true,
   "type": "module",
   "sideEffects": false,
   "scripts": {
      "build": "remix vite:build",
      "deploy": "wrangler pages deploy ./build/client",
      "dev": "remix vite:dev",
      "start": "wrangler pages dev ./build/client --port 3000 --env docker --binding AUTH0_CLIENT_SECRET=$(process.env.AUTH0_CLIENT_SECRET) AUTH0_CLIENT_ID=$(process.env.AUTH0_CLIENT_ID)",
      "lint": "eslint ./app --ext .js,.jsx,.ts,.tsx,.mjs,.cjs",
      "lint:fix": "npm run lint -- --fix",
      "lint:ci": "npm run lint -- --max-warnings 0",
      "knip": "knip",
      "typecheck": "tsc"
   },
   "dependencies": {
      "@chakra-ui/react": "2.8.2",
      "@emotion/cache": "11.11.0",
      "@emotion/react": "11.11.4",
      "@emotion/styled": "11.11.0",
      "@mui/icons-material": "5.15.14",
      "@remix-run/cloudflare": "^2.8.1",
      "@remix-run/cloudflare-pages": "^2.8.1",
      "@remix-run/react": "2.8.1",
      "chakra-react-select": "4.7.6",
      "framer-motion": "11.0.20",
      "miniflare": "^3.20240320.0",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "remix-auth": "3.6.0",
      "remix-auth-auth0": "1.10.0",
      "slate": "0.102.0",
      "slate-history": "0.100.0",
      "slate-react": "0.102.0"
   },
   "devDependencies": {
      "@cloudflare/workers-types": "^4.20240320.1",
      "@remix-run/dev": "2.8.1",
      "@types/react": "18.2.69",
      "@types/react-dom": "18.2.22",
      "@typescript-eslint/eslint-plugin": "7.3.1",
      "@typescript-eslint/parser": "7.3.1",
      "eslint": "8.57.0",
      "eslint-config-prettier": "9.1.0",
      "eslint-import-resolver-typescript": "3.6.1",
      "eslint-plugin-deprecation": "2.0.0",
      "eslint-plugin-import": "2.29.1",
      "eslint-plugin-jsx-a11y": "6.8.0",
      "eslint-plugin-prettier": "5.1.3",
      "eslint-plugin-react": "7.34.1",
      "eslint-plugin-react-hooks": "4.6.0",
      "knip": "5.2.2",
      "prettier": "3.2.5",
      "typescript": "5.4.3",
      "vite": "5.2.4",
      "vite-plugin-checker": "^0.6.4",
      "vite-tsconfig-paths": "4.3.2",
      "wrangler": "^3.37.0"
   }
}
